    "react-router": "^7.8.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bun": "^1.2.21",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
